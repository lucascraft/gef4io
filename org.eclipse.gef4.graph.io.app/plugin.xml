<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.eclipse.gef4.graph.io.app"
         point="org.eclipse.ui.views">
      <view
            class="org.eclipse.gef4.graph.io.app.GraphMLView"
            id="org.eclipse.gef4.graph.io.app"
            name="GraphML View"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.eclipse.gef4.graph.io.app.GrapMLEditor"
            extensions="graphml"
            id="org.eclipse.gef4.graph.io.ui.GraphMLEditor"
            name="GraphML XML Editor">
      </editor>
   </extension>

</plugin>
